{
  "manifest_version": 3,
  "name": "Intent",
  "version": "1.3",
  "description": "A slow productivity app.",
  "permissions": [
    "storage",
    "alarms"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content-script.js"]
    }
  ],
  "icons": {
    "16": "logo32.png",
    "32": "logo32.png",
    "48": "logo32.png",
    "128": "logo256.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}