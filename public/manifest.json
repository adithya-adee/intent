{
  "manifest_version": 3,
  "name": "Intent",
  "version": "1.0",
  "description": "A slow productivity app.",
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "alarms"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ],
  "icons": {
    "16": "logo32.png",
    "32": "logo32.png",
    "48": "logo32.png",
    "128": "logo256.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
